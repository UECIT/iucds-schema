																									<xs:enumeration value="NonTrauma"/>
																									<xs:enumeration value="SceneSafe"/>
																									<xs:enumeration value="SceneUnsafe"/>
																									<xs:enumeration value="Police"/>
																									<xs:enumeration value="Fire"/>
																									<xs:enumeration value="CATALevel1"/>
																									<xs:enumeration value="CATALevel2"/>
																									<xs:enumeration value="CATALevel3"/>
																									<xs:enumeration value="CATALevel4"/>
																									<xs:enumeration value="CATBLevel1"/>
																									<xs:enumeration value="CATBLevel2"/>
																									<xs:enumeration value="CATBLevel3"/>
																									<xs:enumeration value="CATBLevel4"/>
																									<xs:enumeration value="Trapped"/>
																								</xs:restriction>
																							</xs:simpleType>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="origSite">
