																							</xs:annotation>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="userComment" type="xs:string" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Any user comments made as part of this triage line</xs:documentation>
