														<xs:annotation>
															<xs:documentation>Final Disposition reached in this triage.  This may be different to the triageDisposition element if the user has overridden the system.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="finalDispCode" type="xs:string"/>
																<xs:element name="finalDisptext" type="xs:string"/>
																<xs:element name="override" minOccurs="0">
