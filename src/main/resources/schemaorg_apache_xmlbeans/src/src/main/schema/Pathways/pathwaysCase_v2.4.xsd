<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2013 (x64) (http://www.altova.com) by Martin Tapp (Advanced Health and Care) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://nhspathways.org/webservices/pathways/pathwaysCase" targetNamespace="http://nhspathways.org/webservices/pathways/pathwaysCase" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="PathwaysCase">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="dispositionOverrideCode" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="code" type="xs:string"/>
							<xs:element name="reason" type="xs:string" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="appointment" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="appointmentRef" type="xs:string"/>
							<xs:element name="attributableDelay" type="xs:boolean" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="serviceDestination">
					<xs:annotation>
						<xs:documentation>This element will be used to identify organisations which share the same Endpoints</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="name" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The name of the service destination</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="code" type="xs:string">
								<xs:annotation>
									<xs:documentation>A code uniquely identifying the destination service. this should be the same code as looked up from CMS/CLI Mappings/Other Directory of Service as used by the system</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="messageStatus">
					<xs:annotation>
						<xs:documentation>Status to signify if it's a complete message or a partial message</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="PartialExpectingUpdate"/>
							<xs:enumeration value="Complete"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="caseReceiveStart" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>The date and time the call handler answered the phone</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="caseReceiveEnd" type="xs:dateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The date and time the case was dispatched to the system</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="outcome" minOccurs="0">
					<xs:annotation>
						<xs:documentation>This element group contains details of the type of clinical care required.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="title" type="xs:string" minOccurs="0"/>
							<xs:element name="code">
								<xs:annotation>
									<xs:documentation>This field can hold either Output Disposition codes at the end of triage or operational codes where pathways triage was either not performed or overriden.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string"/>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="caseDetails">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="caseRef">
								<xs:annotation>
									<xs:documentation>Unique reference number/id of the case</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="40"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="caseId" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The identifier, as used by staff at the sending site for this particular case. This can be left blank if only caseRef is in use.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="address" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Patient's current location address</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="building">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="55"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="street">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="55"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="locality">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="55"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="town">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="55"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="county">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="55"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="postcode">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="15"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="mapRef" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="longitude" minOccurs="0"/>
													<xs:element name="latitude" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="country" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="name" type="xs:string"/>
													<xs:element name="isoAlpha2">
														<xs:annotation>
															<xs:documentation>Iso Code of the country</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="2"/>
																<xs:maxLength value="2"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="directions" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Any directions to the patientâ€™s current location address</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="512"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="condition">
								<xs:annotation>
									<xs:documentation>Patient's reported current condition.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="512"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="contactDetails">
								<xs:annotation>
									<xs:documentation>It is expected either the caller or patient contact details will be specified.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice maxOccurs="unbounded">
										<xs:element name="caller" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Details of the caller. These should only be populated if the caller is not the patient.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="name" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="0"/>
																<xs:maxLength value="45"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="phone" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The caller's contact phone number</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="prefix" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Prefix to be used with the number. E.g. 18002 for typetalk.</xs:documentation>
																	</xs:annotation>
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:minLength value="0"/>
																			<xs:maxLength value="6"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="number">
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:minLength value="0"/>
																			<xs:maxLength value="23"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="extension" minOccurs="0">
																	<xs:simpleType>
																		<xs:restriction base="xs:string">
																			<xs:minLength value="0"/>
																			<xs:maxLength value="6"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="patientPhone" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The patient's current location phone number</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="prefix" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Prefix to be used with the number. E.g. 18002 for typetalk.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="0"/>
																<xs:maxLength value="6"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="number">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="0"/>
																<xs:maxLength value="23"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="extension" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="0"/>
																<xs:maxLength value="6"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:choice>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="patient" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ref" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Unique reference number/id of the patient</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="40"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="forename" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="45"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="surname" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="45"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="address" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The patient's home address</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="building">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="55"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="street">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="55"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="locality">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="55"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="town">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="55"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="county">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="55"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="postcode">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="15"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="mapRef" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="longitude" minOccurs="0"/>
													<xs:element name="latitude" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="country" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="name" type="xs:string"/>
													<xs:element name="isoAlpha2" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="2"/>
																<xs:maxLength value="2"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="directions" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="512"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="phone" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The patient's home phone number</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="prefix" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Prefix to be used with the number. E.g. 18002 for typetalk.</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="6"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="number">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="23"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="extension" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="0"/>
													<xs:maxLength value="6"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="gender">
								<xs:annotation>
									<xs:documentation>Male,Female,Unknown,Not Specified</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Male"/>
										<xs:enumeration value="Female"/>
										<xs:enumeration value="Unknown"/>
										<xs:enumeration value="Not Specified"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="nationalNumber" minOccurs="0">
								<xs:annotation>
									<xs:documentation>NHS Number for England, CHI for Scotland etc</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="20"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="providerDetails" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="provider" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Patient's provider.  E.g. Their doctor or dentist etc</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="name" type="xs:string">
														<xs:annotation>
															<xs:documentation>Provider's name</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="id">
														<xs:annotation>
															<xs:documentation>The unique id for this provider.  For doctors this should be the GMC number.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string"/>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="providerGroup">
											<xs:annotation>
												<xs:documentation>Patient's provider group.  E.g dental surgery or doctor's surgery</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="name" type="xs:string">
														<xs:annotation>
															<xs:documentation>Provider Group name</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="id">
														<xs:annotation>
															<xs:documentation>The unique id for this provider group.  For surgeries this should be the OCS code.</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string"/>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="type" use="required">
										<xs:annotation>
											<xs:documentation>Currently the type can only be Doctors.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="DOCTOR"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="dateOfBirth" minOccurs="0">
								<xs:complexType>
									<xs:choice>
										<xs:element name="dob">
											<xs:annotation>
												<xs:documentation>Date of Birth</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:date"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="age">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="years" type="xs:int"/>
													<xs:element name="months" type="xs:int" minOccurs="0"/>
													<xs:element name="days" type="xs:int" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:choice>
								</xs:complexType>
							</xs:element>
							<xs:element name="mobilePhone" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="15"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="returnPhone">
					<xs:annotation>
						<xs:documentation>The phone number which the operator should use to return the call. This can be patient's / caller / third number provided during the triage.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="prefix" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Prefix to be used with the number. E.g. 18002 for typetalk.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="6"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="number">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="23"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="extension" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="6"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
