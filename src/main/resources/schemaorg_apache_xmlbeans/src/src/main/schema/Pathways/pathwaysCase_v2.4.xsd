																		<xs:sequence>
																			<xs:element name="productName" type="xs:string"/>
																			<xs:element name="version" type="xs:string"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="callerIsPatient" type="xs:boolean">
														<xs:annotation>
															<xs:documentation>Indicates whether this consultation was carried out with the patient on the phone or a third party</xs:documentation>
														</xs:annotation>
