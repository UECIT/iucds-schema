				<xs:element name="pathwayDetails" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="pathwayTriageDetails">
								<xs:complexType>
									<xs:sequence maxOccurs="unbounded">
										<xs:element name="pathwayTriage">
