													<xs:element name="finish" type="xs:dateTime" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The completion time of this pathway session</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="triageDisposition" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Dispisition reached at the end of the triage, as recommended by the system.</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="initialDispCode" type="xs:string">
