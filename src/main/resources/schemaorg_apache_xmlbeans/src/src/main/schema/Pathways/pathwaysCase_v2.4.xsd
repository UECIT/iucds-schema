																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="T2">
																				<xs:annotation>
																					<xs:documentation>Clinician Out of Hours</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="T3">
																				<xs:annotation>
																					<xs:documentation>Call Handler 999 and 111</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
																			<xs:enumeration value="T4">
																				<xs:annotation>
																					<xs:documentation>Clinician 999 and 111</xs:documentation>
																				</xs:annotation>
																			</xs:enumeration>
